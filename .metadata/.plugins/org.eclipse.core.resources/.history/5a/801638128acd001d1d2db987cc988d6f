package Atividade;

public class Questao3 {
    public static void main(String[] args) {
    	
    	//Sequencia A
        int a = 1;
        int next = a + 2;
        System.out.print(a + ", " + (a+2) + ", ");
        for (int i = 3; i <= 5; i++) {
            a = next;
            next = a + 2;
            System.out.print("Sequencia A" + next + ", "+ "/n");
        }
        System.out.println("...");
    }
    
    
    

}

